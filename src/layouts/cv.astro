---
import Head from "../components/Head.astro";
import Header from "../components/Header.astro";
import Main from "../components/Main.astro";
import Footer from "../components/Footer.astro";

const {title} = Astro.props;
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <Head title={title}/>
  </head>
  <body>
    <Header/>
    <main>
      <slot/>
    </main>
    <Footer/>
  </body>
</html>
